<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="admin_pagecssjs" pattern="/">
        <default key="_controller">PiAppAdminBundle:PageCssJs:index</default>
    </route>

    <route id="admin_pagecssjs_show" pattern="/{id}/show">
        <default key="_controller">PiAppAdminBundle:PageCssJs:show</default>
    </route>

    <route id="admin_pagecssjs_new" pattern="/new">
        <default key="_controller">PiAppAdminBundle:PageCssJs:new</default>
    </route>

    <route id="admin_pagecssjs_create" pattern="/create">
        <default key="_controller">PiAppAdminBundle:PageCssJs:create</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="admin_pagecssjs_edit" pattern="/{id}/edit">
        <default key="_controller">PiAppAdminBundle:PageCssJs:edit</default>
    </route>

    <route id="admin_pagecssjs_update" pattern="/{id}/update">
        <default key="_controller">PiAppAdminBundle:PageCssJs:update</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="admin_pagecssjs_delete" pattern="/{id}/delete">
        <default key="_controller">PiAppAdminBundle:PageCssJs:delete</default>
        <requirement key="_method">post</requirement>
    </route>

</routes>
