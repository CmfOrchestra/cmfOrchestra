	<table id="{{ id_grid }}" >
	    <thead>
	        <tr>
	            <th><a>Id</a></th>
	            <th><a>Route</a></th>
	            <th><a>Url</a></th>
	            <th><a>Rubrique</a></th>
	            <th><a>Layout</a></th>
	            <th><a>Cacheable</a></th>
	            <th><a>Public</a></th>
	            <th><a>Lifetime</a></th>
	            <th><a>Enabled</a></th>
	            <th><a>Actions</a></th>
	        </tr>
	    </thead>
	    <tbody>
	    {% set num_line = 0 %}
	    {% for entity in entities %}
	        <tr {% if(num_line == 0) %}class="odd"{% endif %} id="{{ entity.id }}_row_{{ entity.id }}"  >
	            <td><a href="{{ path('admin_pagebytrans_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
	            <td>	            	
            		<a href="{{ path_url(entity.routeName, {'locale':app.session.locale}) }}" target="_blank" title="{{ path_url(entity.routeName, {'locale':app.session.locale}) }}" class="info-tooltip">
            			{{ entity.routeName }}
            		</a>           	
	            </td>
	            <td>{{ entity.url }}</td>
	            <td>{{ entity.rubrique }}</td>
	            <td>{{ entity.layout }}</td>
	            <td>{{ entity.cacheable }}</td>
	            <td>{{ entity.public }}</td>
	            <td>{{ entity.lifetime }}</td>
	            <td class="enabled">{{ entity.enabled }}</td>
	            <td>
					<a href="{{ path('admin_pagebytrans_show', { 'id': entity.id }) }}" title="{{ 'pi.grid.action.show'|trans }} Trans" class="icon-3 info-tooltip">&nbsp;</a>
					<a href="{{ path('admin_pagebytrans_edit', { 'id': entity.id }) }}" title="{{ 'pi.grid.action.edit'|trans }} Trans" class="icon-1 info-tooltip">&nbsp;</a>
					<a href="{{ path('admin_pagebyblock_show', { 'id': entity.id }) }}" title="{{ 'pi.grid.action.show'|trans }} Block" class="icon-3 info-tooltip">&nbsp;</a>
					<a href="{{ path('admin_pagebyblock_edit', { 'id': entity.id }) }}" title="{{ 'pi.grid.action.edit'|trans }} Block" class="icon-1 info-tooltip">&nbsp;</a>
	            </td>
	        </tr>
	        {% if num_line == 0 %} 
	        	{% set num_line = 1 %} 
	        {% else %}
	        	{% set num_line = 0 %}
	        {% endif %}
	    {% endfor %}
	    </tbody>
	</table>
	
	{% set options_gridtabale = {'grid-name': id_grid, 'grid-type':'simple', 
		'grid-filters': {
			 '1':'Identifiant',
			 '2':'Route', 
			 '3':'Url', 
			 '4':'Rubrique', 
			 '5':'layout' 
			},
		'grid-sorting': { 
				'1':'desc',
			},	
		'grid-actions': {
				'rows_enabled': {'sButtonText':'pi.grid.action.row_enabled', 'route':'admin_pagebytrans_enabledentity_ajax',},
				'rows_disable': {'sButtonText':'pi.grid.action.row_disable', 'route':'admin_pagebytrans_disablentity_ajax'},
				'select_all': {'sButtonText':'pi.grid.action.select_all'},
				'select_none': {'sButtonText':'pi.grid.action.select_none'},
			}				
		}
	%}
	{{ renderJquery('GRID', 'grid-table', options_gridtabale )|raw }}