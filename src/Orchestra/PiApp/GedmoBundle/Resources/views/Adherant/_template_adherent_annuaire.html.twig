<section class="bloc-adh-results">
	<div class="wrap">
		<header>
			<div class="title-border-simple cf">
				<h3 class="tt-5 tt-clr fleft">
				<span>{{ entities|count }}</span> {{ 'Résultat'|translate_plural("Résultats", entities|count) }}</h3>
			</div>
		</header>
			<ul class="results cf">
			{% for entity in entities %}
				{% if entity.CorporationName is defined %}
					{% set id_corporation = entity.id %}
					{% set id_individual = '' %}
				{% elseif entity.Company is defined %}
					{% set id_corporation = '' %}
					{% set id_individual = entity.id %}
				{% endif %}
				<li>
					<span class="innovation"></span>
					<a href="{{ path_url('admin_gedmo_adherent_detail_ajax', {'lang':locale, 'id_individual':id_individual, 'id_corporation':id_corporation}) }}" class="popin fancybox.ajax">
						<figure>
							{% if (entity.media.image.id is defined) and (entity.ArgumentActivity is defined) %}
							<img src="{{ media_url(entity.media.image.id, 'reference', true, entity.media.getUpdatedAt(), 'lamelee_individual_') }}" alt="LA MELEE"/>
							{% elseif (entity.media2.image.id is defined) and (entity.ArgumentActivity is not defined)  %}
							<img src="{{ media_url(entity.media2.image.id, 'reference', true, entity.media2.getUpdatedAt(), 'lamelee_coorporation_') }}" alt="LA MELEE"/>
				     	   	{% endif %}
							<figcaption>
								<p class="light-clr"><span class="tt-clr">
									<strong>
									{% if entity.Name is defined %}
				   					{{ entity.Name }}
				   					{% endif %}		 
									<br>
									{% if entity.Nickname is defined %}
				   					{{ entity.Nickname }}
				   					{% endif %}		
									</strong></span><br>
									{% if entity.Company is defined %}
						   			{{ entity.Company }}
						   			{% elseif entity.CommercialName is defined %}
						   			{{ entity.CommercialName }}
						   			{% endif %}
								</p>
							</figcaption>
						</figure>
					</a>
				</li>
			{% endfor %}
			</ul>	
	</div>
</section>