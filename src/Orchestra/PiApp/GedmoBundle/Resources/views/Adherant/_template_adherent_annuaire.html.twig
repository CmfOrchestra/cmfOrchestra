<section class="bloc-annu-adh cf">
	<div class="wrap">
		<article class="zoom-art">
			<div class="zoom"></div>
			<ul class="cf">
				{% for entity_last in entities_last %}
				{% if entity_last.individual.id is defined %}
					{% set id_corporation = '' %}
					{% set id_individual = entity_last.individual.id %}
				{% elseif entity_last.corporation.id is defined %}
					{% set id_corporation = entity_last.corporation.id %}
					{% set id_individual = '' %}
				{% endif %}
				<li>
					<figure>
						<a href="{{ path_url('admin_gedmo_adherent_detail_ajax', {'lang':locale, 'id_individual':id_individual, 'id_corporation':id_corporation}) }}" class="popin fancybox.ajax">
			     	 	{% if entity_last.individual.media.image.id is defined %}
						<img src="{{ media_url(entity_last.individual.media.image.id, 'reference', true, entity_last.individual.media.getUpdatedAt(), 'lamelee_individual_') }}" alt="LA MELEE"/>
						{% elseif entity_last.corporation.media2.image.id is defined %}
						<img src="{{ media_url(entity_last.corporation.media2.image.id, 'reference', true, entity_last.corporation.media2.getUpdatedAt(), 'lamelee_coorporation_') }}" alt="LA MELEE"/>
			     	   	{% endif %}
						</a>
						<figcaption>
							<p><a href="{{ path_url('admin_gedmo_adherent_detail_ajax', {'lang':locale, 'id_individual':id_individual, 'id_corporation':id_corporation}) }}" class="popin fancybox.ajax"> <span class="bold">
								{% if entity_last.individual.Name is defined %}
			   					{{ entity_last.individual.Name }}
			   					{% elseif entity_last.corporation.Name is defined %}
			   					{{ entity_last.corporation.Name }}
			   					{% endif %}		   					
			   					
			   					{% if entity_last.individual.Nickname is defined %}
			   					{{ entity_last.individual.Nickname }}
			   					{% elseif entity_last.corporation.Nickname is defined %}
			   					{{ entity_last.corporation.Nickname }}
			   					{% endif %}		
											</span><br>
				   				{% if entity_last.individual.Company is defined %}
					   			{{ entity_last.individual.Company }}
					   			{% elseif entity_last.corporation.CommercialName is defined %}
					   			{{ entity_last.corporation.CommercialName }}
					   			{% endif %}
								</a>
							</p>
						</figcaption>
					</figure>
				</li>
				{% endfor %}
			</ul>
		</article>
	</div>
</section>

