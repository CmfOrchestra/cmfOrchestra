{% if entity.pageurl.getRouteName() is defined %}
	{% set url = path_url(entity.pageurl.getRouteName(),{'locale':locale}) %}
	<a href="{{ entity.url }}" >
{% elseif entity.url is not empty %}
	{% set url = entity.url %}
	<a href="http://{{ entity.url|replace({'http://':'', 'https://':''}) }}" >
{% endif %}
		{{ entity.translate(locale).getcontent()|raw }}
{% if url is defined %}
	</a>
{% endif %}