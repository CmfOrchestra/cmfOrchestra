<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="admin_gedmo_individual" pattern="/">
        <default key="_controller">PiAppGedmoBundle:Individual:index</default>
    </route>

    <route id="admin_gedmo_individual_show" pattern="/{id}/show">
        <default key="_controller">PiAppGedmoBundle:Individual:show</default>
    </route>

    <route id="admin_gedmo_individual_new" pattern="/new">
        <default key="_controller">PiAppGedmoBundle:Individual:new</default>
    </route>

    <route id="admin_gedmo_individual_create" pattern="/create">
        <default key="_controller">PiAppGedmoBundle:Individual:create</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="admin_gedmo_individual_edit" pattern="/{id}/edit">
        <default key="_controller">PiAppGedmoBundle:Individual:edit</default>
    </route>

    <route id="admin_gedmo_individual_update" pattern="/{id}/update">
        <default key="_controller">PiAppGedmoBundle:Individual:update</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="admin_gedmo_individual_delete" pattern="/{id}/delete">
        <default key="_controller">PiAppGedmoBundle:Individual:delete</default>
        <requirement key="_method">post</requirement>
    </route>

</routes>
