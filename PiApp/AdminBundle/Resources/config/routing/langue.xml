<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="admin_langue" pattern="/">
        <default key="_controller">PiAppAdminBundle:Langue:index</default>
    </route>

    <route id="admin_langue_show" pattern="/{id}/show">
        <default key="_controller">PiAppAdminBundle:Langue:show</default>
    </route>

    <route id="admin_langue_new" pattern="/new">
        <default key="_controller">PiAppAdminBundle:Langue:new</default>
    </route>

    <route id="admin_langue_create" pattern="/create">
        <default key="_controller">PiAppAdminBundle:Langue:create</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="admin_langue_edit" pattern="/{id}/edit">
        <default key="_controller">PiAppAdminBundle:Langue:edit</default>
    </route>

    <route id="admin_langue_update" pattern="/{id}/update">
        <default key="_controller">PiAppAdminBundle:Langue:update</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="admin_langue_delete" pattern="/{id}/delete">
        <default key="_controller">PiAppAdminBundle:Langue:delete</default>
        <requirement key="_method">post</requirement>
    </route>

</routes>
