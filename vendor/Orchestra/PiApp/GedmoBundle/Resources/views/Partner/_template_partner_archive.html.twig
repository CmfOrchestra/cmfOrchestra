<section class="bloc-annu-part cf">
	<div class="wrap">
	{% for pos,infos in entities %}
		<h3 class="tt-6">{{ infos.name }}</h3>
		<ul class="cf">
		{% for entities_by_cat in infos.partner %}
			{% if entities_by_cat.pageurl.getRouteName() is defined %}
				{% set url = path_url(entities_by_cat.pageurl.getRouteName(),{'locale':locale}) %}
			{% elseif entities_by_cat.url is not empty %}
				{% set url = entities_by_cat.url %}
			{% else %}
				{% set url = "#" %}		
			{% endif %}			
			<li>
				<a href="{{ url }}" class="mask" target="_blank" style="left: -300px;">
					<div class="text">
						<p>
						{{ entities_by_cat.translate(locale).getTitle()|striptags }}
						</p>
					</div>	
					<span></span>
					<span class="more"></span>
				</a>										
				<figure>
					{% if entities_by_cat.media.image.id is defined %}
					<img src="{{ media_url(entities_by_cat.media.image.id, 'reference', true, entities_by_cat.media.getUpdatedAt(), 'lamelee_partner_') }}" width="120" height="120" alt="LA MELEE"/>
					{% endif %}
				</figure>
			</li>
		{% endfor %}
		</ul>
	{% endfor %}
	</div>
</section>