<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="fos_user.user_manager.default" class="FOS\UserBundle\Propel\UserManager" public="false">
            <argument type="service" id="security.encoder_factory" />
            <argument type="service" id="fos_user.util.username_canonicalizer" />
            <argument type="service" id="fos_user.util.email_canonicalizer" />
            <argument>%fos_user.model.user.class%</argument>
            <argument>%fos_user.model.user.propel_class%</argument>
        </service>

        <service id="fos_user.propel.validator_initializer" class="FOS\UserBundle\Propel\ProxyInitializer" public="false">
            <tag name="validator.initializer" />
        </service>
    </services>

</container>
